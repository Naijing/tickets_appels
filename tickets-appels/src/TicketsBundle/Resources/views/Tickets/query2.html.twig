{% extends "TicketsBundle::layout.html.twig" %}

{% block body %}

    <!--Main!!!!Important!!!!!!!!!!!!!!!!!!!!!!! Right side column. Contains the navbar and content of the page -->
    <aside class="right-side">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>Tickets Appels</h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <!-- <li class="active">Blank page</li> -->
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
    <div class="row">
    <div class="col-xs-12">
    <div class="box">
    <div class="box-header">
        <h3 class="box-title"> {{ title}} </h3>
    </div>
    <div class="box-body">
    <div class="row margin">

        <table class="table table-bordered table-hover">
            <thead>
            <tr>
                <th>Compte facturé</th>
                <th>N° Facture</th>
                <th>N° abonné</th>
                <th>Date </th>
                <th>Heure </th>
                <th>Durée/volume réel </th>
                <th>Durée/volume facturé </th>
                <th>Type </th>
            </tr>
            </thead>
            <tbody>
            {% for t in res %}
                <tr>
                    <td>{{t.compteFacture}}</td>
                    <td>{{t.numFacture}}</td>
                    <td>{{t.numAbonne}}</td>
                    <td>{{t.date}}</td>
                    <td>{{t.heure}}</td>
                    <td>{{t.dureeReel}}</td>
                    <td>{{t.dureeFacture}}</td>
                    <td>{{t.type}}</td>

                </tr>
            {% endfor %}
            </tbody>
            <tfoot>
            <tr>
                <th>Compte facturé</th>
                <th>N° Facture</th>
                <th>N° abonné</th>
                <th>Date </th>
                <th>Heure </th>
                <th>Durée/volume réel </th>
                <th>Durée/volume facturé </th>
                <th>Type </th>
            </tr>
            </tfoot>
        </table>


    </div><!-- /.box-body -->
{% endblock %}